## affiche un point grimpette si conditions réunies
mkgmap:line2poitype=mid & (highway=path | highway=track) & (mtb:scale:uphill=5) {name 'grimpette'}	[0x5e00 resolution 23 continue]

(highway=path | highway=footway | highway=track) & mtb:scale=* {delete sac_scale; delete smoothness}
(highway=path | highway=footway | highway=track) & sac_scale=* {delete smoothness}

(highway=path | highway=footway ) & mtb:scale=0 {add difficult=facile}
(highway=path | highway=footway | highway=track) & (mtb:scale=1|smoothness=horrible  ) {add difficult=technique}
(highway=path | highway=footway | highway=track) & (mtb:scale=2 | mtb:scale=3 | sac_scale=mountain_hiking | smoothness=very_horrible ) {add difficult=difficile}
(highway=path | highway=footway | highway=track ) & (smoothness=impassable| mtb:scale=4 | mtb:scale=5 | mtb:scale=6 | sac_scale=difficult_alpine_hiking | sac_scale=alpine_hiking|sac_scale=demanding_alpine_hiking | sac_scale=demanding_mountain_hiking) {add difficult=impassable}

mkgmap:line2poitype=mid & ((highway=path | highway=footway ) & (difficult=facile)) {name 'sentier facile'} [0x01d01 resolution 22-23 continue] #facile
mkgmap:line2poi=true & ((highway=path | highway=footway ) & (difficult=facile)) {name 'sentier facile'} [0x01d05 resolution 23-23 continue] #facile
mkgmap:line2poi=true & ((highway=path | highway=footway ) & (difficult=facile)) {name 'sentier facile'} [0x02a01 resolution 24] #facile

